;Demo
		ORG 2000H
		
G_INIT_LCD:	EQU 3000H
G_CLEAR_GBUF:	EQU 3003H
G_CLEAR_GR_LCD:	EQU 3006H
G_CLEAR_TXT_LCD:	EQU 3009H
G_SET_GR_MODE:	EQU 300CH
G_SET_TXT_MODE:	EQU 300FH
G_DRAW_BOX:	EQU 3012H
G_DRAW_LINE:	EQU 3015H
G_DRAW_CIRCLE:	EQU 3018H
G_DRAW_PIXEL:	EQU 301BH
G_FILL_BOX:	EQU 301EH
G_FILL_CIRCLE: EQU 3021H
G_PLOT_TO_LCD:	EQU 3024H
G_PRINT_STRING:	EQU 3027H
G_PRINT_CHARS:	EQU 302AH
G_DELAY_US:	EQU 302DH
G_DELAY_MS:	EQU 3030H
G_SET_BUF_CLEAR:	EQU 3033H
G_SET_BUF_NO_CLEAR:	EQU 3036H
		
START:	
		CALL G_INIT_LCD
AGAIN:	
		RST 08H
		CP 0
		JP Z, DO_BOX
		CP 1
		JP Z, DO_LINES
		CP 2
		JP Z, DO_TEXT
		CP 3
		JP Z, DO_SETXY
		CP 4
		JP Z, DO_SET_CLRBUF
		CP 5
		JP Z, DO_RES_CLRBUF
        CP 6
        JP Z, DO_MAD
        CP 7
        JP Z, DO_MAD_FAST
        CP 8
        JP Z, DO_CLEAR_TXT
        CP 9
        JP Z, DO_CLEAR_GR
        CP 0AH
        JP Z, DO_CIRCLES
        CP 0BH
        JP Z, DO_TEXT2
        CP 0CH
        JP Z, DO_FILL_BOX
        CP 0DH
        JP Z, DO_FILL_CIRCLE
		CP 12H
		RET Z
		JR AGAIN
		
DO_BOX:	
		CALL G_SET_GR_MODE
		LD BC, 0505H
		LD DE, 7A3AH
		CALL G_DRAW_BOX
		CALL G_PLOT_TO_LCD
		JP AGAIN
		
DO_FILL_BOX:	
		CALL G_SET_GR_MODE
		LD BC, 0520H
		LD DE, 153FH
		CALL G_FILL_BOX
		LD BC, 2030H
		LD DE, 353FH
		CALL G_FILL_BOX
		LD BC, 4010H
		LD DE, 553FH
		CALL G_FILL_BOX
		LD BC, 6018H
		LD DE, 753FH
		CALL G_FILL_BOX
		CALL G_PLOT_TO_LCD
		JP AGAIN
		
DO_LINES:
		CALL G_SET_GR_MODE
		LD BC, 0505H
		LD DE, 7A3AH
		CALL G_DRAW_LINE
		LD BC, 053AH
		LD DE, 7A05H
		CALL G_DRAW_LINE
		CALL G_PLOT_TO_LCD
		JP AGAIN

DO_CIRCLES:
		CALL G_SET_GR_MODE
		LD BC, 4020H
		LD E, 10H
		CALL G_DRAW_CIRCLE
		LD BC, 103AH
		LD E, 05H
		CALL G_DRAW_CIRCLE
		CALL G_PLOT_TO_LCD
		JP AGAIN

DO_FILL_CIRCLE:
		CALL G_SET_GR_MODE
		LD BC, 4020H
		LD E, 10H
		CALL G_FILL_CIRCLE
		LD BC, 103AH
		LD E, 05H
		CALL G_FILL_CIRCLE
		CALL G_PLOT_TO_LCD
		JP AGAIN

DO_TEXT:
		CALL G_SET_TXT_MODE
		XOR A
		CALL G_PRINT_STRING
		DB "First Line Here*",0
		LD A, 01H
		CALL G_PRINT_STRING
		DB "Second here..",0
		LD A, 02H
		CALL G_PRINT_STRING
		DB "Ok Third :-)",0
		LD A, 03H
		CALL G_PRINT_STRING
		DB "Bottom to toppy!",0
		JP AGAIN

DO_TEXT2:
		CALL G_SET_TXT_MODE
		LD BC,0000H
		LD HL,TEXT
		CALL G_PRINT_CHARS
		LD BC,0101H
		LD HL,TEXT
		CALL G_PRINT_CHARS
		LD BC,0202H
		LD HL,TEXT
		CALL G_PRINT_CHARS
		LD BC,0303H
		LD HL,TEXT
		CALL G_PRINT_CHARS
		JP AGAIN
TEXT:	DB "Hello TEC",0

DO_SETXY:
		CALL G_SET_GR_MODE
		LD BC, 1010H
		CALL G_DRAW_PIXEL
		LD BC, 2020H
		CALL G_DRAW_PIXEL
		LD BC, 3030H
		CALL G_DRAW_PIXEL
		CALL G_PLOT_TO_LCD
		JP AGAIN
		
DO_SET_CLRBUF:
		CALL G_SET_BUF_CLEAR
        JP AGAIN
		
DO_RES_CLRBUF:
		CALL G_SET_BUF_NO_CLEAR
		JP AGAIN

DO_CLEAR_GR:
        CALL G_CLEAR_GR_LCD
        JP AGAIN

DO_CLEAR_TXT:
        CALL G_CLEAR_TXT_LCD
        JP AGAIN
    
DO_MAD:
		CALL G_SET_GR_MODE
		CALL G_SET_BUF_NO_CLEAR
        LD HL, MAD_DATA
MAD_LOOP:
        LD A, (HL)
        CP 0FFH
        RET Z
        LD B, A
        INC HL
        LD C, (HL)
        INC HL
        LD D, (HL)
        INC HL
        LD E, (HL)
        INC HL
        PUSH HL
		CALL G_DRAW_LINE
        CALL G_PLOT_TO_LCD
        POP HL
        JR MAD_LOOP

DO_MAD_FAST:
		CALL G_SET_GR_MODE
		CALL G_SET_BUF_CLEAR
        LD HL, MAD_DATA
MAD_LOOP1:
        LD A, (HL)
        CP 0FFH
        JR Z, MDEND
        LD B, A
        INC HL
        LD C, (HL)
        INC HL
        LD D, (HL)
        INC HL
        LD E, (HL)
        INC HL
        PUSH HL
		CALL G_DRAW_LINE
        POP HL
        JR MAD_LOOP1
MDEND:
        CALL G_PLOT_TO_LCD
        RET

